feature akhn {
  script guru;
  sub Ka.gm RaVirama.gm by KaViramaRa.gm;
  sub Na.gm RaVirama.gm by NaViramaRa.gm;
  sub Rra.gm HaVirama.gm by Rha.gm;
  sub Na.gm HaVirama.gm by NaHa.gm;
  sub La.gm HaVirama.gm by LaHa.gm;
  

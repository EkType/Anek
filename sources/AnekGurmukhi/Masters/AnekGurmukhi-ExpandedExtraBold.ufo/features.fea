  sub Ka.gm Virama.gm Ra.gm by KaViramaRa.gm;
  sub Na.gm Virama.gm Ra.gm by NaViramaRa.gm;
  sub Rra.gm Virama.gm Ha.gm by Rha.gm;
  sub Na.gm Virama.gm Ha.gm by NaHa.gm;
  sub La.gm Virama.gm Ha.gm by LaHa.gm;
  
} akhn;

feature blwf {

  sub Virama.gm Ha.gm by PairinHa.gm;
  sub Virama.gm Va.gm by PairinVa.gm;
} lookup2;

script guru; 
  sub Ra.gm Virama.gm by PairinRa.gm;
  sub Ha.gm Virama.gm by PairinHa.gm;
  sub Va.gm Virama.gm by PairinVa.gm;


} blwf;

